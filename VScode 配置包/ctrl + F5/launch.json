{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      
      "args": ["/C","${fileDirname}\\${fileBasenameNoExtension}.exe","&","pause","-fexec-charset=gbk"],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "e:/code/C",
      // "program": "e:/code/C/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "D:/Mingw64/mingw64/bin/gdb.exe",
      
      "program":"C:\\Windows\\System32\\cmd.exe",


      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}